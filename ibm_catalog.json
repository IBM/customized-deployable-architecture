{
    "products": [
        {
            "name": "deploy-arch-ibm-gm-test-slz",
            "label": "Test SLZ",
            "product_kind": "solution",
            "tags": ["network", "network_vpc"],
            "keywords": ["custom", "vpc", "secure", "baby"],
            "short_description": "Customized example for the landing zone solution using parameters and a json override.",
            "long_description": "This example is designed to illustrate how to customize a solution to fit your needs.  This particular customization is a simple o ne.  The value of the deployment region has been set to 'us-east' to show to to satisfy a potential custom requirement.",
            "offering_docs_url": "https://raw.githubusercontent.com/IBM/customized-deployable-architecture/main/README.md",
            "offering_icon_url": "data:image/svg+xml;base64,",
            "support_details": "This is a demonstration and no official support is given.",
            "flavors": [
                {
                    "label": "Baby SLZ",
                    "name": "babyslz",
                    "install_type": "fullstack",
                    "working_directory": "solutions/custom-deployable-arch",
                    "usage_template": "module \"custom_arch\" {\n    source           = \"https://cm.globalcatalog.cloud.ibm.com/api/v1-beta/offering/source//${{workingDirectory}}?archive=tgz&catalogID=${{catalogID}}&flavor=${{flavor}}&kind=${{kind}}&name=${{name}}&version=${{version}}\"\n    prefix           = var.prefix\n    ssh_key          = var.ssh_key\n    ibmcloud_api_key = var.ibmcloud_api_key\n}\n\n",
                    "compliance": {
                        "authority": "scc-v3",
                        "profiles":[
                            {
                              "profile_name": "IBM Cloud for Financial Services",
                              "profile_version": "1.1.0"
                            }
                        ]
                    },
                    "configuration": [
                        {
                            "key": "ssh_key",
                            "required": true
                        },
                        {
                            "key": "ibmcloud_api_key",
                            "required": true,
                            "type": "password"
                        },
                        {
                            "key": "prefix",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "outputs": [
                        {
                            "description": "ID of the IBM Cloud Schematics workspace. Returns null if not ran in Schematics",
                            "key": "schematics_workspace_id"
                        },
                        {
                            "description": "prefix used in this infrastructure",
                            "key": "prefix"
                        }
                    ],
                    "iam_permissions": [
                        {
                            "role_crns": [
                                "crn:v1:bluemix:public:iam::::serviceRole:Manager"
                            ],
                            "service_name": "appid"
                        },
                        {
                            "role_crns": [
                                "crn:v1:bluemix:public:iam::::role:Administrator"
                            ],
                            "service_name": "iam-identity"
                        },
                        {
                            "role_crns": [
                                "crn:v1:bluemix:public:iam::::serviceRole:Manager"
                            ],
                            "service_name": "kms"
                        },
                        {
                            "role_crns": [
                                "crn:v1:bluemix:public:iam::::serviceRole:Manager",
                                "crn:v1:bluemix:public:iam::::role:Administrator"
                            ],
                            "service_name": "is.vpc"
                        }
                    ],
                    "architecture": {
                        "descriptions": "",
                        "features": [
                            {
                                "title": "Customized",
                                "description": "Yes"
                            },
                            {
                                "title": "Separate VPC for workloads",
                                "description": "Yes"
                            },
                            {
                                "title": "Increases security with Key Management",
                                "description": "Yes"
                            },
                            {
                                "title": "Uses Floating IP address for access through the public internet",
                                "description": "Yes"
                            }
                        ],
                        "diagrams": [
                            {
                                "diagram": {
                                    "caption": "Custom Edge variation",
                                    "url": "https://raw.githubusercontent.com/IBM/customized-deployable-architecture/main/images/baby-slz.svg",
                                    "metadata": []
                                },
                                "description": "Baby SLZ variation of VSI on VPC landing zone requires at least 2 CPUs and 4 GB of memory.\n\nVSI on VPC landing zone is implemented with virtual servers. It is a single Virtual Private Cloud (VPC). IBM Cloud services are configured for storage and key management."
                            }
                        ]
                    }
                }
            ]
        }       
    ]
