{
  "name": "${BlueprintName}",
  "tags": ["sb-test"],
  "source": {
    "source_type": "git_hub",
    "git": {
      "git_repo_url": "https://github.com/IBM/customized-deployable-architecture",
      "git_repo_folder": "/examples/app/full/blueprint/sb-demo.yaml",
      "git_branch": "main"
    }
  },
  "inputs": [
    {
      "name": "ibmcloud_api_key",
      "value": "${APIKEY}"
    },
    {
      "name": "prefix",
      "value": "sb-tst"
    },
    {
      "name": "ssh_key",
      "value": "${SSHKEY}"
    }
  ],
  "description": "TEST: deployable architecture blueprint made from Solution Builder",
  "resource_group": "${ResourceGroup}",
  "location": "${Location}"
}
